{
  "name": "@capawesome/capacitor-sqlite-drizzle",
  "version": "0.0.1",
  "description": "Drizzle ORM adapter for the Capawesome SQLite plugin.",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "dist/"
  ],
  "author": "Robin Genz <mail@robingenz.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capawesome-team/capacitor-sqlite-drivers.git"
  },
  "bugs": {
    "url": "https://github.com/capawesome-team/capacitor-sqlite-drivers/issues"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/capawesome-team/"
    },
